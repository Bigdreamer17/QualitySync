# Server
PORT=8000
NODE_ENV=development

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# Resend Email
RESEND_API_KEY=your_resend_api_key
EMAIL_FROM=QualitySync <onboarding@resend.dev>

# Frontend URL (for email links)
FRONTEND_URL=http://localhost:5173

# CORS Allowed Origins (comma-separated for multiple origins)
# If not set, falls back to FRONTEND_URL
ALLOWED_ORIGINS=http://localhost:5173,https://www.estifanosfm.dev,https://estifanosfm.dev

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
